
config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy0'
	option htmode 'HT20'
	option txpower '17'
	option chanbw '20'
	option hwmode '11ng'
	option disabled '0'
	option channel '1'

config wifi-iface 'ap_0'
	option device 'radio0'
	option ifname 'wlan0'
	option network 'lan'
	option mode 'ap'
	option hidden '0'
	option isolate '0'
	option maxassoc '35'
	option disabled '0'
	option key 'password'
	option ssid 'VT-RACHEL-1'
	option encryption 'psk'

config wifi-iface 'ap_1'
	option device 'radio0'
	option ifname 'wlan0-3'
	option mode 'ap'
	option network 'guest'
	option isolate '1'
	option maxassoc '5'
	option disabled '0'
	option key 'password2'
	option ssid 'Public-VT-RACHEL-1'
	option encryption 'psk'

config wifi-iface ah_0
	option device 'radio0'
	option ifname 'wlan0-1'
	option mode 'mesh'
	option mesh_id 'vtmesh'
	option network 'mesh_0'
	option encryption 'none'
	option disabled '0'
	option mesh_fwding '0'

config wifi-iface 'sta_0'
	option device 'radio0'
	option ifname 'wlan0-2'
	option mode 'sta'
	option network 'wan'
	option ssid 'host-ssid'
	option disabled '1'
	option key 'host-password'
	option encryption 'psk'

